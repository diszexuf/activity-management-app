{
  "name": "activity-management-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate:api && vite",
    "build": "npm run generate:api && vite build",
    "preview": "vite preview",
    "clean:api": "rimraf src/api/generated",
    "generate:api": "npm run clean:api && openapi-generator-cli generate -i ./../openapi.yaml -g typescript-axios -o src/api/generated",
    "regenerate:api": "npm run generate:api"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.9",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "vue": "^3.5.24",
    "vuetify": "^3.11.6"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^6.0.1",
    "rimraf": "^6.1.2",
    "unplugin-fonts": "^1.4.0",
    "vite": "^7.2.4"
  }
}
